{
  "name": "picturebook",
  "version": "2.0.0-beta.12",
  "description": "Opinionated Storybook Setup",
  "main": "dist/index.js",
  "browser": "dist/picturebook.web.js",
  "repository": "https://github.com/obartra/picturebook",
  "author": "Oscar Bartra",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"{{src,test,sampleFolder}/**/,}*.js\"",
    "test": "jest",
    "lint": "eslint --ext .js --cache .",
    "build": "webpack --mode production --config webpack.build.config.js && babel src -d dist --copy-files",
    "build:watch": "yarn build --watch",
    "test:watch": "yarn test --watch",
    "start": "yarn && yarn build && cd sampleFolder && yarn && yarn start",
    "test:app": "cd sampleFolder && yarn test --env chrome,firefox,ie11,edge,safari,iphone7",
    "update-deps": "yarn upgrade --exact --latest && cd sampleFolder && yarn upgrade --exact --latest && cd .. && yarn ci",
    "ci": "yarn build && yarn lint && yarn flow check && yarn test"
  },
  "dependencies": {
    "fs-extra": "7.0.0",
    "lodash": "4.17.11",
    "node-sauce-connect": "4.5.0",
    "pixelmatch": "4.0.2",
    "png-crop": "0.0.2",
    "pngjs": "3.3.3",
    "rimraf": "2.6.2",
    "selenium-server-standalone-jar": "3.14.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-regenerator": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.2",
    "@storybook/react": "3.4.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-require-context-hook": "1.0.0",
    "clean-webpack-plugin": "0.1.19",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.24.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.82.0",
    "jest": "23.6.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "peerDependencies": {
    "nightwatch": ">=0.9.x",
    "react": ">= 15.x",
    "react-dom": ">= 15.x"
  }
}
