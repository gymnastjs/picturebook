{
  "name": "picturebook",
  "version": "2.0.0-beta.8",
  "description": "Opinionated Storybook Setup",
  "main": "dist/index.js",
  "browser": "dist/picturebook.web.js",
  "repository": "https://github.com/obartra/picturebook",
  "author": "Oscar Bartra",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"{{src,test,sampleFolder}/**/,}*.js\"",
    "test": "jest",
    "lint": "eslint --ext .js --cache .",
    "build": "webpack --mode production --config webpack.build.config.js && babel src -d dist --copy-files",
    "build:watch": "yarn build --watch",
    "test:watch": "yarn test --watch",
    "start": "yarn && yarn build && cd sampleFolder && yarn && yarn start",
    "test:app": "cd sampleFolder && yarn test --env chrome,firefox,ie11,edge,safari,iphone7",
    "ci": "yarn lint && yarn flow && yarn test"
  },
  "dependencies": {
    "fs-extra": "7.0.0",
    "lodash": "4.17.10",
    "node-sauce-connect": "4.5.0",
    "pixelmatch": "4.0.2",
    "png-crop": "0.0.2",
    "pngjs": "3.3.3",
    "rimraf": "2.6.2",
    "selenium-download": "2.0.13",
    "selenium-server-standalone-jar": "3.13.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "7.0.0",
    "@storybook/react": "3.4.10",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.79.1",
    "jest": "23.5.0",
    "prettier": "1.14.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0"
  },
  "peerDependencies": {
    "nightwatch": ">=0.9.x",
    "react": ">= 15.x",
    "react-dom": ">= 15.x"
  }
}
